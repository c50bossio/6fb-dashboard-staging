{
  "$schema": "https://railway.app/railway.schema.json",
  "build": {
    "builder": "DOCKERFILE",
    "dockerfilePath": "Dockerfile"
  },
  "variables": {
    "CODER_ACCESS_URL": "https://${{RAILWAY_PUBLIC_DOMAIN}}",
    "CODER_HTTP_ADDRESS": "0.0.0.0:${{PORT}}",
    "CODER_TELEMETRY": "false",
    "PORT": "7080"
  }
}