{
  "name": "6fb-ai-agent-system",
  "version": "2.0.0",
  "description": "Comprehensive AI-powered barbershop management system with intelligent business coaching",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "start:test": "NODE_ENV=test next start -p 3000",
    "start:production": "NODE_ENV=production next start -p 3000",
    "lint": "npm run lint:js && npm run lint:py",
    "lint:js": "next lint",
    "lint:py": "ruff check . && black --check .",
    "lint:fix": "next lint --fix && ruff check . --fix && black .",
    "format": "prettier --write . && black .",
    "format:check": "prettier --check .",
    "type-check": "tsc --noEmit",
    "test": "npm run test:unit && npm run test:integration && npm run test:e2e",
    "test:unit": "jest --config jest.config.js",
    "test:unit:watch": "jest --config jest.config.js --watch",
    "test:unit:coverage": "jest --config jest.config.js --coverage",
    "test:integration": "jest --config jest.integration.config.js",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:performance": "playwright test __tests__/performance/",
    "test:security": "playwright test __tests__/security/",
    "test:accessibility": "playwright test __tests__/e2e/ --grep accessibility",
    "test:visual": "playwright test --grep visual",
    "test:backend": "cd .. && python -m pytest __tests__/backend/ -v",
    "test:backend:coverage": "cd .. && python -m pytest __tests__/backend/ -v --cov=. --cov-report=html",
    "test:ai": "cd .. && python -m pytest __tests__/backend/test_ai_agent_system.py -v",
    "test:load": "artillery run __tests__/load/artillery-config.yml",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:e2e && npm run test:backend",
    "db:migrate": "node scripts/migrate.js",
    "db:seed": "node scripts/seed.js",
    "db:seed:test": "NODE_ENV=test node scripts/seed.js",
    "db:seed:performance": "NODE_ENV=performance node scripts/seed-performance.js",
    "db:seed:load-test": "NODE_ENV=load-test node scripts/seed-load-test.js",
    "db:reset": "npm run db:migrate && npm run db:seed",
    "analyze": "npm run analyze:bundle && npm run analyze:deps",
    "analyze:bundle": "ANALYZE=true npm run build",
    "analyze:deps": "npx depcheck && npm audit",
    "security:audit": "npm audit && safety check && bandit -r . -f json",
    "lighthouse": "lhci autorun",
    "docker:dev": "docker-compose -f docker-compose.dev.yml up",
    "docker:test": "docker-compose -f docker-compose.test.yml up --abort-on-container-exit",
    "docker:prod": "docker-compose -f docker-compose.prod.yml up",
    "clean": "rm -rf .next node_modules coverage playwright-report test-results",
    "reset": "npm run clean && npm install",
    "prepare": "husky install"
  },
  "dependencies": {
    "next": "^14.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "next-auth": "^4.24.5",
    "chart.js": "^4.4.0",
    "react-chartjs-2": "^5.2.0",
    "date-fns": "^2.30.0",
    "clsx": "^2.0.0",
    "tailwindcss": "^3.3.6",
    "autoprefixer": "^10.4.16",
    "postcss": "^8.4.32",
    "@tailwindcss/forms": "^0.5.7",
    "@headlessui/react": "^1.7.17",
    "@heroicons/react": "^2.0.18",
    "framer-motion": "^10.16.16",
    "react-hook-form": "^7.48.2",
    "@hookform/resolvers": "^3.3.2",
    "zod": "^3.22.4",
    "axios": "^1.6.2",
    "swr": "^2.2.4",
    "socket.io-client": "^4.7.4",
    "react-hot-toast": "^2.4.1",
    "react-query": "^3.39.3",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/react": "^18.2.38",
    "@types/react-dom": "^18.2.17",
    "typescript": "^5.3.2",
    "eslint": "^8.54.0",
    "eslint-config-next": "^14.0.0",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-import": "^2.29.0",
    "prettier": "^3.1.0",
    "prettier-plugin-tailwindcss": "^0.5.7",
    "husky": "^8.0.3",
    "lint-staged": "^15.1.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "@testing-library/react": "^14.1.2",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/user-event": "^14.5.1",
    "msw": "^2.0.8",
    "@playwright/test": "^1.40.0",
    "playwright": "^1.40.0",
    "artillery": "^2.0.3",
    "@faker-js/faker": "^8.3.1",
    "@axe-core/playwright": "^4.8.2",
    "pa11y": "^7.0.0",
    "@lhci/cli": "^0.12.0",
    "cross-env": "^7.0.3",
    "dotenv": "^16.3.1",
    "nodemon": "^3.0.2",
    "concurrently": "^8.2.2",
    "@next/bundle-analyzer": "^14.0.0",
    "webpack-bundle-analyzer": "^4.10.1",
    "@storybook/react": "^7.6.3",
    "@storybook/addon-essentials": "^7.6.3",
    "@storybook/addon-a11y": "^7.6.3",
    "chromatic": "^10.0.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{css,md,json}": [
      "prettier --write"
    ],
    "*.py": [
      "black",
      "ruff check --fix"
    ]
  },
  "jest": {
    "testEnvironment": "jsdom",
    "setupFilesAfterEnv": [
      "<rootDir>/__tests__/setup/jest.setup.js"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/.next/",
      "<rootDir>/node_modules/",
      "<rootDir>/__tests__/e2e/",
      "<rootDir>/__tests__/integration/",
      "<rootDir>/__tests__/performance/",
      "<rootDir>/__tests__/security/"
    ],
    "collectCoverageFrom": [
      "components/**/*.{js,jsx,ts,tsx}",
      "app/**/*.{js,jsx,ts,tsx}",
      "lib/**/*.{js,jsx,ts,tsx}",
      "utils/**/*.{js,jsx,ts,tsx}",
      "!**/*.d.ts",
      "!**/node_modules/**",
      "!**/.next/**"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    },
    "moduleNameMapping": {
      "^@/(.*)$": "<rootDir>/$1",
      "^@/components/(.*)$": "<rootDir>/components/$1",
      "^@/lib/(.*)$": "<rootDir>/lib/$1",
      "^@/utils/(.*)$": "<rootDir>/utils/$1"
    },
    "testTimeout": 10000
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/6fb-ai-agent-system.git"
  },
  "keywords": [
    "barbershop",
    "booking",
    "ai",
    "business-intelligence",
    "react",
    "next.js",
    "fastapi",
    "typescript",
    "python"
  ],
  "author": "6FB Development Team",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/your-org/6fb-ai-agent-system/issues"
  },
  "homepage": "https://github.com/your-org/6fb-ai-agent-system#readme"
}