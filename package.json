{
  "name": "6fb-ai-agent-system",
  "version": "1.0.0",
  "description": "Enterprise barbershop platform with AI agents",
  "main": "index.js",
  "scripts": {
    "dev": "next dev -p 9999",
    "build": "next build",
    "start": "next start -p 9999",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "check:production": "node scripts/verify-production-readiness.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "test:unit": "jest --testMatch='**/__tests__/**/*.test.js' --testMatch='**/?(*.)+(spec|test).js'",
    "test:integration": "jest --testMatch='**/?(*.)+(integration|e2e).test.js'",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:e2e",
    "test:e2e": "playwright test",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:smoke": "bash scripts/run-e2e-tests.sh --smoke",
    "test:e2e:critical": "bash scripts/run-e2e-tests.sh --critical-only",
    "test:e2e:full": "bash scripts/run-e2e-tests.sh --full",
    "test:e2e:booking": "bash scripts/run-e2e-tests.sh --suites booking",
    "test:e2e:payment": "bash scripts/run-e2e-tests.sh --suites payment",
    "test:e2e:notifications": "bash scripts/run-e2e-tests.sh --suites notifications",
    "test:e2e:analytics": "bash scripts/run-e2e-tests.sh --suites analytics",
    "test:e2e:visual": "bash scripts/run-e2e-tests.sh --suites visual",
    "test:e2e:errors": "bash scripts/run-e2e-tests.sh --suites errors",
    "test:e2e:mobile": "playwright test --project='Mobile Chrome' --project='Mobile Safari'",
    "test:e2e:cross-browser": "playwright test --project=chromium --project=firefox --project=webkit",
    "test:security": "npm run test:security:quick && npm run test:security:comprehensive",
    "test:security:quick": "playwright test tests/security/ --grep='@quick'",
    "test:security:comprehensive": "playwright test tests/security/",
    "test:performance": "playwright test tests/performance/",
    "test:accessibility": "playwright test --project=accessibility-tests",
    "test:visual": "playwright test --project=visual-tests",
    "claude:health": "node -e \"console.log('Health check script would verify all services are running')\"",
    "claude:setup": "npm install && npx playwright install",
    "claude:test-setup": "npm run claude:health && npm run test:e2e:smoke",
    "claude:validate": "npm run lint && npm run type-check && npm run build",
    "claude:pre-commit": "npm run claude:validate && npm run test:all",
    "report:generate": "node tests/e2e-test-runner.js --generate-report-only",
    "report:open": "open playwright-report/index.html",
    "report:serve": "npx playwright show-report",
    "ci:test": "npm run test:ci && npm run test:e2e:critical",
    "ci:full": "npm run claude:pre-commit && npm run test:e2e:full",
    "dev:backend": "python fastapi_backend.py",
    "dev:full": "concurrently \"npm run dev\" \"npm run dev:backend\"",
    "dev:docker": "bash docker-dev-start.sh"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^2.0.4",
    "@ai-sdk/openai": "^2.0.15",
    "@anthropic-ai/sdk": "^0.24.3",
    "@fullcalendar/core": "^6.1.19",
    "@fullcalendar/daygrid": "^6.1.19",
    "@fullcalendar/interaction": "^6.1.19",
    "@fullcalendar/list": "^6.1.19",
    "@fullcalendar/react": "^6.1.19",
    "@fullcalendar/resource-timegrid": "^6.1.19",
    "@fullcalendar/resource-timeline": "^6.1.19",
    "@fullcalendar/rrule": "^6.1.19",
    "@fullcalendar/timegrid": "^6.1.19",
    "@google/generative-ai": "^0.24.1",
    "@headlessui/react": "^1.7.17",
    "@heroicons/react": "^2.0.18",
    "@next/bundle-analyzer": "^14.0.3",
    "@playwright/test": "^1.40.0",
    "@radix-ui/react-alert-dialog": "^1.0.5",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-tabs": "^1.0.4",
    "@radix-ui/react-toast": "^1.1.5",
    "@sendgrid/mail": "^8.1.5",
    "@sentry/nextjs": "^7.82.0",
    "@stripe/react-stripe-js": "^3.9.0",
    "@stripe/stripe-js": "^2.1.11",
    "@supabase/ssr": "^0.0.10",
    "@supabase/supabase-js": "^2.38.4",
    "@vercel/edge-config": "^1.4.0",
    "ai": "^5.0.15",
    "bcryptjs": "^2.4.3",
    "canvas-confetti": "^1.9.3",
    "chart.js": "^4.5.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "date-fns": "^2.30.0",
    "dotenv": "^17.2.1",
    "framer-motion": "^10.16.5",
    "googleapis": "^156.0.0",
    "ical-generator": "^9.0.0",
    "ioredis": "^5.7.0",
    "jose": "^5.1.3",
    "limiter": "^3.0.0",
    "lucide-react": "^0.294.0",
    "luxon": "^3.7.1",
    "next": "^14.0.3",
    "nodemailer": "^7.0.5",
    "openai": "^4.20.1",
    "otplib": "^12.0.1",
    "posthog-js": "^1.260.1",
    "pusher": "^5.2.0",
    "qrcode": "^1.5.4",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.48.2",
    "recharts": "^3.1.2",
    "rrule": "^2.8.1",
    "stripe": "^14.5.0",
    "tailwind-merge": "^2.0.0",
    "tailwindcss-animate": "^1.0.7",
    "twilio": "^5.8.0",
    "uuid": "^11.1.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@babel/core": "^7.23.3",
    "@babel/preset-env": "^7.23.3",
    "@babel/preset-react": "^7.23.3",
    "@jest/globals": "^29.7.0",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/jest": "^29.5.8",
    "@types/node": "^20.9.0",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "autoprefixer": "^10.4.16",
    "babel-jest": "^29.7.0",
    "concurrently": "^8.2.2",
    "eslint": "^8.54.0",
    "eslint-config-next": "^14.0.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "postcss": "^8.4.31",
    "tailwindcss": "^3.3.5",
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/6fb-ai-agent-system.git"
  },
  "keywords": [
    "barbershop",
    "ai-agents",
    "booking-system",
    "nextjs",
    "supabase",
    "stripe",
    "enterprise"
  ],
  "author": "6FB AI Team",
  "license": "MIT",
  "jest": {
    "testEnvironment": "jsdom",
    "setupFilesAfterEnv": [
      "<rootDir>/test-utils/jest.setup.js"
    ],
    "moduleNameMapping": {
      "^@/(.*)$": "<rootDir>/$1"
    },
    "testMatch": [
      "**/__tests__/**/*.test.js",
      "**/?(*.)+(spec|test).js"
    ],
    "collectCoverageFrom": [
      "components/**/*.{js,jsx}",
      "lib/**/*.{js,jsx}",
      "pages/**/*.{js,jsx}",
      "app/**/*.{js,jsx}",
      "!**/*.config.js",
      "!**/node_modules/**"
    ],
    "coverageReporters": [
      "text",
      "lcov",
      "html"
    ],
    "coverageDirectory": "coverage"
  },
  "eslintConfig": {
    "extends": [
      "next/core-web-vitals"
    ],
    "rules": {
      "@next/next/no-html-link-for-pages": "off"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
